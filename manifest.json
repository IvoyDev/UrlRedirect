{
  "manifest_version": 2,

  "name": "Url Redirect",
  "short_name": "URedir",
  "description": "Redirect current tab urls",
  "version": "0.7.4.24",
  "icons": { "16": "img/Quora16.png",
           "48": "img/Quora48.png",
          "128": "img/Quora.png" },

  "browser_action": {
    "default_icon": "img/Quora.png",
    "default_title": "Url Redirect",
	"default_popup": "popup.html"
  },
  "permissions": [
    "tabs"
  ]
}